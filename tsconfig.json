{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2015",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "ScoutDesign": ["./ScoutDesign"],
      "CreateEvent": ["./src/modules/createEvent/createEventForm"],
      "data": ["./src/data"],
      "ScoutDesign/*": ["./ScoutDesign/*"],
      "CreateEvent/*": ["./src/modules/createEvent/createEventForm/*"],
      "data/*": ["./src/data/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
