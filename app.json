{
  "expo": {
    "name": "ScoutTrek",
    "slug": "scout-trek-native",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "scouttrek",
            "project": "scouttrek",
            "authToken": "aee5440c28cd4466a32e2d29c61724ebcddffb3172d0491fb3d481b038519af5"
          }
        }
      ]
    },
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/ScoutTrekIcon.png",
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.scouttrek.kalebdavenport",
      "buildNumber": "1.0.29"
    },
    "android": {
      "useNextNotificationsApi": true,
      "versionCode": 12,
      "permissions": [
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "READ_PHONE_STATE",
        "VIBRATE",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCy38WoJFJEn3gr7EEc1OAWcO-Xsslilbs"
        }
      },
      "package": "com.wowyourclient.scouttrek",
      "googleServicesFile": "./google-services.json"
    },
    "splash": {
      "backgroundColor": "#2C3148",
      "resizeMode": "cover",
      "image": "./assets/images/loadingImage.png"
    },
    "description": "Testing the publishing feature of Expo",
    "notification": {
      "icon": "./assets/images/Icon.png",
      "color": "#F29959",
      "iosDisplayInForeground": true
    }
  }
}
