{
  "expo": {
    "name": "ScoutTrek",
    "slug": "ScoutTrekNative",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json"
    },
    "splash": {
      "backgroundColor": "#2C3148",
      "resizeMode": "cover",
      "image": "./assets/images/loadingImage.png"
    },
    "description": "",
    "notification": {
      "icon": "./assets/images/Icon.png",
      "color": "#F29959",
      "iosDisplayInForeground": true,
      "androidMode": "collapse",
      "androidCollapsedTitle": "ScoutTrek Reminders"
    }
  }
}
